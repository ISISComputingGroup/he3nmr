record(stringout, "$(P)CMD:SP")
{
    field(DTYP, "stream")
    field(OUT, "@he3nmr.proto runCommandStr $(PORT)")
}

record(waveform, "$(P)CMDWF:SP")
{
    field(DTYP, "stream")
    field(INP, "@he3nmr.proto runCommandStr $(PORT)")
    field(FTVL, "CHAR")
    field(NELM, "256")	
}

record(longin, "$(P)CELL:VAL") 
{
    field(DTYP, "stream")
    field(INP, "@he3nmr.proto getCellValue $(PORT)")
	field(SCAN, "Event")
	field(EVNT, "Update")
	info(archive, "VAL")
}

record(waveform, "$(P)NMR:FNAME:CUST:SP")
{
    field(DTYP, "stream")
    field(INP, "@he3nmr.proto NMRCustomFilename $(PORT)")
    field(FTVL, "CHAR")
    field(NELM, "256")	
}

record(bo, "$(P)NMR:FNAME:INT:SP")
{
    field(DTYP, "stream")
    field(OUT, "@he3nmr.proto NMRInternalFilename $(PORT)")
}

